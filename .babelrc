{
  "presets": [
    "@babel/env",
    "@babel/react"
  ],
  "env": {
    "production": {
      "plugins": [
        [
          "transform-react-remove-prop-types",
          {
            "mode": "remove",
            "removeImport": true
          }
        ]
      ]
    }
  },
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ]
}
